<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXCheckBox?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="singerMusic" prefWidth="1022.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kr.or.ddit.clap.main.MusicMainController">
   <children>
      <VBox fx:id="mainBox">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="5.0">
               <padding>
                  <Insets left="30.0" />
               </padding>
               <children>
                  <Label fx:id="lb_total" text="발매 곡 (총 12개)">
                     <font>
                        <Font name="YDIYGO330" size="13.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="45.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" spacing="1.0">
               <children>
                  <JFXCheckBox fx:id="cb_main" checkedColor="#9c0000" onAction="#mainCheck" prefHeight="35.0" prefWidth="39.0" text=" " />
                  <JFXButton onAction="#btnMainPlay" ripplerFill="#9c0000" text="듣기">
                     <graphic>
                        <FontAwesomeIcon iconName="PLAY_CIRCLE_ALT" size="20" />
                     </graphic>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                     <font>
                        <Font name="-윤고딕340" size="12.0" />
                     </font>
                  </JFXButton>
                  <JFXButton onAction="#btnMainAdd" ripplerFill="#9c0000" text="추가">
                     <graphic>
                        <FontAwesomeIcon iconName="PLUS" size="20" />
                     </graphic>
                     <font>
                        <Font name="-윤고딕330" size="12.0" />
                     </font>
                  </JFXButton>
                  <JFXButton onAction="#btnMainPut" ripplerFill="#9c0000" text="담기">
                     <graphic>
                        <FontAwesomeIcon iconName="FOLDER_ALT" size="20" />
                     </graphic>
                     <font>
                        <Font name="-윤고딕330" size="12.0" />
                     </font>
                  </JFXButton>
               </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" spacing="20.0">
               <children>
                  <Label text="번호">
                     <font>
                        <Font name="-윤고딕320" size="12.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="70.0" />
                     </HBox.margin>
                  </Label>
                  <Label text="곡정보">
                     <font>
                        <Font name="-윤고딕320" size="12.0" />
                     </font>
                     <padding>
                        <Insets left="65.0" />
                     </padding>
                  </Label>
                  <Label text="듣기">
                     <font>
                        <Font name="-윤고딕320" size="12.0" />
                     </font>
                     <padding>
                        <Insets left="300.0" />
                     </padding>
                  </Label>
                  <Label text="추가">
                     <font>
                        <Font name="-윤고딕320" size="12.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <Label text="담기">
                     <font>
                        <Font name="-윤고딕320" size="12.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <Label text="뮤비">
                     <font>
                        <Font name="-윤고딕320" size="12.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <padding>
                  <Insets left="60.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets left="40.0" />
   </padding>
</StackPane>

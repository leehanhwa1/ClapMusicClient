<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXCheckBox?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="stackpane" prefWidth="1022.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kr.or.ddit.clap.view.chartmenu.period.PeriodController">
   <children>
      <VBox fx:id="mainBox" spacing="9.0">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="5.0">
               <padding>
                  <Insets left="45.0" />
               </padding>
               <children>
                  <JFXButton fx:id="btn_Interior" onAction="#interiorChart" prefWidth="50.0" ripplerFill="#9c0000" text="국내">
                     <HBox.margin>
                        <Insets left="45.0" />
                     </HBox.margin>
                     <font>
                        <Font name="-윤고딕330" size="15.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="btn_Foreign" onAction="#foreignChart" prefWidth="50.0" ripplerFill="#9c0000" text="국외">
                     <font>
                        <Font name="-윤고딕330" size="15.0" />
                     </font>
                  </JFXButton>
                  <Label fx:id="la_Date" text="Label">
                     <font>
                        <Font name="-윤고딕350" size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="230.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" spacing="1.0">
               <children>
                  <JFXCheckBox fx:id="cb_main" checkedColor="#9c0000" onAction="#mainCheck" prefHeight="35.0" prefWidth="39.0" text=" " />
                  <JFXButton onAction="#btnMainPlay" ripplerFill="#9c0000" text="듣기">
                     <graphic>
                        <FontAwesomeIcon iconName="PLAY_CIRCLE_ALT" size="20" />
                     </graphic>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                     <font>
                        <Font name="-윤고딕330" size="15.0" />
                     </font>
                  </JFXButton>
                  <JFXButton onAction="#btnMainAdd" ripplerFill="#9c0000" text="추가">
                     <graphic>
                        <FontAwesomeIcon iconName="PLUS" size="20" />
                     </graphic>
                     <font>
                        <Font name="-윤고딕330" size="15.0" />
                     </font>
                  </JFXButton>
                  <JFXButton onAction="#btnMainPut" ripplerFill="#9c0000" text="담기">
                     <graphic>
                        <FontAwesomeIcon iconName="FOLDER_ALT" size="20" />
                     </graphic>
                     <font>
                        <Font name="-윤고딕330" size="15.0" />
                     </font>
                  </JFXButton>
               </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" spacing="20.0">
               <children>
                  <Label text="순위">
                     <font>
                        <Font name="-윤고딕320" size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="70.0" />
                     </HBox.margin>
                  </Label>
                  <Label text="곡정보">
                     <font>
                        <Font name="-윤고딕320" size="15.0" />
                     </font>
                     <padding>
                        <Insets left="65.0" />
                     </padding>
                  </Label>
                  <Label text="듣기">
                     <font>
                        <Font name="-윤고딕320" size="15.0" />
                     </font>
                     <padding>
                        <Insets left="290.0" />
                     </padding>
                  </Label>
                  <Label text="추가">
                     <font>
                        <Font name="-윤고딕320" size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="0.0" />
                     </HBox.margin>
                  </Label>
                  <Label text="담기">
                     <font>
                        <Font name="-윤고딕320" size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Label>
                  <Label text="뮤비">
                     <font>
                        <Font name="-윤고딕320" size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <padding>
                  <Insets left="60.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="0.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <StackPane.margin>
            <Insets left="60.0" />
         </StackPane.margin>
      </VBox>
   </children>
</StackPane>
